        type="text/css" />
    <link href="../../../Content/themes/nftheme/theme-minimalist-square.css" rel="stylesheet"
        type="text/css" />
    <script type="text/javascript">

        (function ($) {

            $.fn.initialText = function (options) {

                // Create some defaults, extending them with any options that were provided
                var settings = $.extend({

            }, options);

            return this.each(function () {
                this.style.color = options.colorBlur;
                if (this.value != null) this.value = options.text;
                $(this).focus(function () {


                    this.style.color = options.colorFocus;
                    if (this.value == options.text) this.value = "";


                });

                $(this).blur(function () {



                    this.style.color = options.colorBlur;


                    if (this.value == "") this.value = options.text;
                 });
                // Tooltip plugin code here

            });

        };
    })(jQuery);

    $(function () {
        $('#username').initialText({ text: 'Username', colorBlur: '#a9a9a9', colorFocus: '#505050' });
        $('#pwd').initialText({ text: '******', colorBlur: '#a9a9a9', colorFocus: '#505050' });


        var updateCaption = function (slider) {
            var cap = slider.$currentPage.find('.caption').html();

            $('#current-caption').html(cap).fadeIn(200);
        };
        $('#slider').anythingSlider({
            enableStartStop: false,
            buildNavigation: false,
            // *********** Callbacks ***********
            // Callback when the plugin finished initializing
            onInitialized: function (e, slider) { },

            // Callback before slide animates
            onSlideBegin: function (e, slider) {
                //$('#current-caption').fadeOut(200);
            },

            // Callback when slide completes - no event variable!
            onSlideComplete: function (slider) { updateCaption(slider); }

        }
            );

        $('#externalNav a').click(function () {
            var slide = $(this).attr('href').substring(1);
            $('#slider').anythingSlider(slide);
            return false;
        });
    });
        
    </script>

    @if (_addthisEnabled)
    {
        
    <script type="text/javascript">        var addthis_config = { "data_track_addressbar": true };</script>
    <script type="text/javascript" src="@("http://s7.addthis.com/js/250/addthis_widget.js#pubid="+ GSet.Read("GLOBAL_ADDTHIS_USERNAME"))"></script>    
    }
        
        @ViewBag.HeadAditionalLast

    @HeadLoader.LoadHeadDataFromViews(this.ViewContext.Controller.ControllerContext)

